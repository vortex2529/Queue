#include <iostream>
#include <deque>
using namespace std;

int main() {

  // creation
  deque<int> dq;

  // insertion
  dq.push_front(5);
  dq.push_front(10);
  dq.push_back(15);
  dq.push_back(20);

  // removal
  dq.pop_front();
  // size
  cout<<"Size :"<<dq.size()<<endl;


  // empty & traversal
  while(!dq.empty()){
    cout<<dq.front()<<" ";
    dq.pop_front();
  }



  
  return 0;
}
