#include <iostream>
#include<queue>
using namespace std;

int main() {
  // creation
  queue<int> q;

  // Insertion
  q.push(1);
  q.push(2);
  q.push(3);
  q.push(4);

  // Removal
  q.pop();

  // Front
  cout<<"Front Element: "<<q.front()<<endl;

  // back
  cout<<"Back Element: "<<q.back()<<endl;

  // size
  cout<<"size: "<<q.size()<<endl;

  // empty 
  cout<<"empty or not: "<<q.empty()<<endl; 

  // print
  cout<<"Printing Queue: ";
  while(!q.empty()){
    cout<<q.front()<<" ";
    q.pop();
  }
  cout<<endl;





  return 0;
}
